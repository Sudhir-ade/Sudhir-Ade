#include <stdio.h>

int main() {
    int arr[] = {1, 2, 8, 9, 12, 46, 76, 82, 15, 20, 30};
    int size = sizeof(arr) / sizeof(arr[0]);

    int count[10] = {0};      // We will use index 1 to 9 only

    // Count multiples
    for (int i = 0; i < size; i++) {
        for (int j = 1; j <= 9; j++) {
            if (arr[i] % j == 0) {
                count[j]++;
            }
        }
    }

    // Print result in dictionary format
    printf("{");
    for (int j = 1; j <= 9; j++) {
        printf("%d: %d", j, count[j]);
        if (j < 9) {
            printf(", ");
        }
    }
    printf("}");

    return 0;
}
