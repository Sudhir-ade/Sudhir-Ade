#include <stdio.h>

int main() {
    int a, n;
    scanf("%d", &a);

    // Find largest odd number <= a
    if (a % 2 == 0)
        n = a - 1;
    else
        n = a;

    // Print odd numbers from 1 to n
    for (int i = 1; i <= n; i++) {
        int odd = 2 * i - 1;
        printf("%d", odd);

        if (i < n) {
            printf(", ");
        }
    }

    return 0;
}
